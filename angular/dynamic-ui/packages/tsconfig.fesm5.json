{
  "compilerOptions": {
    "baseUrl": "../",
    "paths": {
      "@bpw-ui/*": [
        "dist-package/@bpw-ui/*"
      ]
    },
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "../dist-package/@bpw-ui/",
    "removeComments": false,
    "rootDir": ".",
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [],
    "types": []
  }
}